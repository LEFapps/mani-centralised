{
  "name": "mani-pona",
  "version": "0.0.1",
  "description": "A centralized (AWS based) implementation of SuMSy ledgers",
  "main": "src/client/index.js",
  "bin": {
    "sumsy": "./dist/cli.js"
  },
  "scripts": {
    "build": "rollup -c",
    "test": "LOG_PREFIX=sumsy NODE_DEBUG=sumsy* LOG_LEVEL=error JEST_CIRCUS=1 LEDGER=manipona jest --bail",
    "test:debug": "LOG_PREFIX=sumsy NODE_DEBUG=sumsy:* LOG_LEVEL=debug JEST_CIRCUS=1 LEDGER=manipona jest --bail",
    "test:dynamodb": "NODE_ENV=development sls dynamodb start --inMemory",
    "offline": "npm run build && sls offline start --noAuth --stage offline",
    "deploy": "npm run build && sls deploy",
    "deploy:dev": "npm run deploy -- -s dev",
    "deploy:test": "npm run deploy -- -s test"
  },
  "keywords": [
    "sumsy",
    "mani",
    "currency",
    "client",
    "payments"
  ],
  "author": "Thomas Goorden <tg@eenwereldmetlef.be>",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/preset-env": "^7.12.7",
    "@faykah/core": "^0.4.0",
    "@faykah/first-names-en": "^0.1.0",
    "@faykah/last-names-en": "^0.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "apollo-server-testing": "^2.19.0",
    "aws-sdk-mock": "^5.1.0",
    "babel-jest": "^26.6.3",
    "jest": "^26.6.3",
    "jest-circus": "^26.6.3",
    "jest-dynalite": "^3.2.3",
    "node-fetch": "^2.6.1",
    "rollup-plugin-hashbang": "^2.2.2",
    "serverless-appsync-simulator": "^0.8.1",
    "serverless-dynamodb-local": "^0.2.39",
    "serverless-offline": "^8.2.0",
    "serverless-stack-output": "^0.2.3",
    "serverless-webpack": "^5.3.5",
    "source-map-support": "^0.5.19",
    "text-encoding-utf-8": "^1.0.2"
  },
  "dependencies": {
    "apollo-server": "^3.1.2",
    "apollo-server-core": "^3.4.0",
    "apollo-server-lambda": "^3.1.2",
    "aws-sdk": "^2.973.0",
    "babel-plugin-graphql-tag": "^3.1.0",
    "currency.js": "^2.0.3",
    "dom-storage": "^2.1.0",
    "dynamo-plus": "^1.7.1",
    "graphql": "^15.5.0",
    "graphql-scalars": "^1.6.1",
    "graphql-tools": "^7.0.2",
    "inquirer": "^7.3.3",
    "lodash": "^4.17.20",
    "openpgp": "^5.0.0-6",
    "server-log": "file:./log_module",
    "serverless-dotenv-plugin": "^3.3.0",
    "sha1": "^1.1.1"
  }
}
